//You can use mouseX and mouseY as parameters to create a variety of colors.

draw = function() {
    fill(mouseY, 0, 255);
    stroke(0, 0, 0);
    ellipse(mouseX, mouseY, 400, 50);
};

//ORRRRR

draw = function() {
    fill(mouseX, mouseX, 255);
    stroke(0, 0, 0);
    ellipse(mouseX, mouseY, 60, 50);
};

//ETC.
